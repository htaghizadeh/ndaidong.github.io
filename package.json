{
  "version": "1.1.4",
  "name": "web-builder",
  "homepage": "https://ndaidong.github.io",
  "description": "Web builder tool for creating your own homepage like About.me",
  "repository": {
    "type": "git",
    "url": "https://github.com/ndaidong/ndaidong.github.io"
  },
  "author": "@ndaidong",
  "engines": {
    "node": ">= 8.5"
  },
  "main": "./server.js",
  "scripts": {
    "start": "DEBUG=web-builder:* PORT=7856 node server",
    "lint": "eslint .",
    "pretest": "npm run lint && npm run setup && npm run build",
