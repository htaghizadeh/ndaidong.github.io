    "eslint": "^4.10.0",
    "eslint-config-goes": "^1.0.0",
    "express": "^4.16.2",
    "html-minifier": "^3.5.6",
    "http-server": "^0.10.0",
    "mkdirp": "^0.5.1",
    "postcss": "^6.0.14",
    "postcss-clean": "^1.1.0",
    "postcss-cssnext": "^3.0.2",
    "postcss-import": "^11.0.0",
    "pug": "^2.0.0-rc.4",
